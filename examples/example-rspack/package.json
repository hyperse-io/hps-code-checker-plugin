{
  "name": "@hyperse/example-rspack",
  "version": "0.0.1",
  "homepage": "https://github.com/hyperse-io/hps-code-checker-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperse-io/hps-code-checker-plugin",
    "directory": "packages/hps-code-checker-webpack"
  },
  "author": {
    "name": "Hyperse",
    "url": "https://github.com/hyperse-io"
  },
  "type": "module",
  "license": "SEE LICENSE IN FILE 'LICENSE'",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "bin/",
    "dist/",
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build:test": "node --import @hyperse/ts-node/register ./build.ts",
    "clean": "rimraf --no-glob ./dist ./_release ./coverage ./tsconfig.tsbuildinfo",
    "lint": "eslint .",
    "test": "run-s test-unit",
    "test-unit": "vitest run",
    "test-coverage": "vitest run --coverage",
    "test-unit-watch": "vitest watch --ui",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@dimjs/utils": "2.0.0",
    "@hyperse/eslint-config-hyperse": "^1.4.8",
    "@hyperse/hps-code-checker-rspack-plugin": "workspace:*",
    "@rspack/core": "^1.5.2",
    "@types/node": "^24.3.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "less": "^4.4.1",
    "less-loader": "^12.3.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_release/package"
  }
}
